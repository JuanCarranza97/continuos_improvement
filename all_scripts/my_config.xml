<?xml version="1.0"?>
<concurrency_test>
    <test name="Concurrency Base 8 hrs">
        <command_line>stress-platform -t 08:00:00</command_line>
        <delay_time>00:00:30</delay_time>
        <estimated_time>08:00:00</estimated_time>
        <timeout>08:30:00</timeout>
    </test>
    <test name="Concurrency Base 24 Hrs">
        <command_line>stress-platform -t 24:00:00</command_line>
        <delay_time>00:00:10</delay_time>
        <estimated_time>24:00:00</estimated_time>
        <timeout>24:30:00</timeout>
    </test>
</concurrency_test>